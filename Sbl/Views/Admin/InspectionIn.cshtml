@section Scripts {
    <script>

        $("#VehicleId").select2();
        $("#AssociateId").select2();

        jQuery('.sbl-datepicker').datepicker({
            autoclose: true,
            todayHighlight: true,
            startView: 0,
            format: "dd M yyyy"
        });


        SetGrid();
        function SetGrid() {
            var cols = 30;
            var rows = 30;
            var width = 20;
            var height = 20;
            $('#side1').empty();
            for (y = 0; y <= (rows - 1) ; y++) {
                for (x = 0; x <= (cols - 1) ; x++) {
                    $('#side1').append("<div class=\"gridcell " + "x" + x + "y" + y + "\" data-cols=\"" + cols + "\" data-rows=\"" + rows + "\" data-x=\"" + x + "\" data-y=\"" + y + "\" data-width=\"" + width + "\" data-height=\"" + height + "\" style=\"width:" + (width) + "px; height:" + (height) + "px;\"></div>");
                }
            }
            $('#side2').empty();
            for (y = 0; y <= (rows - 1) ; y++) {
                for (x = 0; x <= (cols - 1) ; x++) {
                    $('#side2').append("<div class=\"gridcell " + "x" + x + "y" + y + "\" data-cols=\"" + cols + "\" data-rows=\"" + rows + "\" data-x=\"" + x + "\" data-y=\"" + y + "\" data-width=\"" + width + "\" data-height=\"" + height + "\" style=\"width:" + (width) + "px; height:" + (height) + "px;\"></div>");
                }
            }
            $('#front').empty();
            for (y = 0; y <= (rows - 1) ; y++) {
                for (x = 0; x <= (cols - 1) ; x++) {
                    $('#front').append("<div class=\"gridcell " + "x" + x + "y" + y + "\" data-cols=\"" + cols + "\" data-rows=\"" + rows + "\" data-x=\"" + x + "\" data-y=\"" + y + "\" data-width=\"" + width + "\" data-height=\"" + height + "\" style=\"width:" + (width) + "px; height:" + (height) + "px;\"></div>");
                }
            }
            $('#back').empty();
            for (y = 0; y <= (rows - 1) ; y++) {
                for (x = 0; x <= (cols - 1) ; x++) {
                    $('#back').append("<div class=\"gridcell " + "x" + x + "y" + y + "\" data-cols=\"" + cols + "\" data-rows=\"" + rows + "\" data-x=\"" + x + "\" data-y=\"" + y + "\" data-width=\"" + width + "\" data-height=\"" + height + "\" style=\"width:" + (width) + "px; height:" + (height) + "px;\"></div>");
                }
            }
            $('#windshield').empty();
            for (y = 0; y <= (rows - 1) ; y++) {
                for (x = 0; x <= (cols - 1) ; x++) {
                    $('#windshield').append("<div class=\"gridcell " + "x" + x + "y" + y + "\" data-cols=\"" + cols + "\" data-rows=\"" + rows + "\" data-x=\"" + x + "\" data-y=\"" + y + "\" data-width=\"" + width + "\" data-height=\"" + height + "\" style=\"width:" + (width) + "px; height:" + (height) + "px;\"></div>");
                }
            }
        }

        // side1
        $(document.body).on("click", "#side1 .gridcell", function () {
            if ($(this).hasClass("g1")) {
                $(this).removeClass("g1");
                $(this).addClass("g2");
            }
            else {
                if ($(this).hasClass("g2")) {
                    $(this).removeClass("g2");
                    $(this).addClass("g3");
                }
                else {
                    if ($(this).hasClass("g3")) {
                        $(this).removeClass("g3");
                        $(this).addClass("g4");
                    }
                    else {
                        if ($(this).hasClass("g4")) {
                            $(this).removeClass("g4");
                            $(this).addClass("g5");
                        }
                        else {
                            if ($(this).hasClass("g5")) {
                                $(this).removeClass("g5");
                                $(this).addClass("g6");
                            }
                            else {
                                if ($(this).hasClass("g6")) {
                                    $(this).removeClass("g6");
                                    $(this).addClass("g7");
                                }
                                else {
                                    if ($(this).hasClass("g7")) {
                                        $(this).removeClass("g7");
                                    }
                                    else {
                                        $(this).addClass("g1");
                                    }
                                }
                            }
                        }
                    }
                }
            }

            GetGridSide1();
        });

        function GetGridSide1() {
            var save1 = "";
            $("#side1 .g1").each(function () {
                var x = $(this).data("x");
                var y = $(this).data("y");
                save1 = save1 + "g1" + "x" + x + "y" + y + "es";
            });
            $("#side1 .g2").each(function () {
                var x = $(this).data("x");
                var y = $(this).data("y");
                save1 = save1 + "g2" + "x" + x + "y" + y + "es";
            });
            $("#side1 .g3").each(function () {
                var x = $(this).data("x");
                var y = $(this).data("y");
                save1 = save1 + "g3" + "x" + x + "y" + y + "es";
            });
            $("#side1 .g4").each(function () {
                var x = $(this).data("x");
                var y = $(this).data("y");
