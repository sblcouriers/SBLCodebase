@using Sbl.Helpers
@model Sbl.Models.InspectionInViewModel

@{
    ViewBag.Title = "Inspection In";
}


<style>
    .gridcell {
        border: 1px dotted #cccccc;
        color: #f1f1f1;
        display: inline-block;
        float: left;
    }

    .g1 {
        background-color: red;
        opacity: 0.8;
    }

    .g2 {
        background-color: darkblue;
        opacity: 0.8;
    }

    .g3 {
        background-color: gold;
        opacity: 0.8;
    }

    .g4 {
        background-color: dimgrey;
        opacity: 0.8;
    }

    .g5 {
        background-color: green;
        opacity: 0.8;
    }

    .g6 {
        background-color: purple;
        opacity: 0.8;
    }

    .g7 {
        background-color: darkturquoise;
        opacity: 0.8;
    }
</style>

<div class="page-wrapper">
    <div class="container-fluid">
        <div class="row page-titles">
            <div class="col-md-5 align-self-center">
                <h4 class="text-themecolor">Inspection In</h4>
            </div>
            <div class="col-md-7 align-self-center text-right">
                <div class="d-flex justify-content-end align-items-center">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="~/admin/dashboard">Dashboard</a></li>
                        <li class="breadcrumb-item"><a href="~/admin/inspectionlist">Inspections</a></li>
                        <li class="breadcrumb-item active">Inspection In</li>
                    </ol>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
