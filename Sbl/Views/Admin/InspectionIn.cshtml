                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="custom-control custom-checkbox">
                                                        @Html.CheckBoxFor(m => m.TyrePressuresCheckedIn, new { @class = "custom-control-input" })
                                                        @Html.LabelFor(m => m.TyrePressuresCheckedIn, new { @class = "custom-control-label" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="custom-control custom-checkbox">
                                                        @Html.CheckBoxFor(m => m.SeatbeltConditionAndOperationIn, new { @class = "custom-control-input" })
                                                        @Html.LabelFor(m => m.SeatbeltConditionAndOperationIn, new { @class = "custom-control-label" })
                                                    </div>
