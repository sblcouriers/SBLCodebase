                                        <h4 class="text-success">Operational Checks</h4>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="custom-control custom-checkbox">
                                                        @Html.CheckBoxFor(m => m.LightsIndicatorsHornIn, new { @class = "custom-control-input" })
                                                        @Html.LabelFor(m => m.LightsIndicatorsHornIn, new { @class = "custom-control-label" })
                                                    </div>
