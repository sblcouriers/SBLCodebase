                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="custom-control custom-checkbox">
                                                        @Html.CheckBoxFor(m => m.RadioIn, new { @class = "custom-control-input" })
                                                        @Html.LabelFor(m => m.RadioIn, new { @class = "custom-control-label" })
