                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="custom-control custom-checkbox">
                                                        @Html.CheckBoxFor(m => m.CigaretteLighterIn, new { @class = "custom-control-input" })
                                                        @Html.LabelFor(m => m.CigaretteLighterIn, new { @class = "custom-control-label" })
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="custom-control custom-checkbox">
                                                        @Html.CheckBoxFor(m => m.JackAndToolsPresentIn, new { @class = "custom-control-input" })
                                                        @Html.LabelFor(m => m.JackAndToolsPresentIn, new { @class = "custom-control-label" })
