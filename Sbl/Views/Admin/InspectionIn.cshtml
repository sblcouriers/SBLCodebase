                                                        @Html.LabelFor(m => m.EngineOilLevelIn, new { @class = "custom-control-label" })
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="custom-control custom-checkbox">
                                                        @Html.CheckBoxFor(m => m.BrakeFluidLevelIn, new { @class = "custom-control-input" })
