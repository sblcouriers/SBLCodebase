        display: inline-block;
        float: left;
    }

    .g1 {
        background-color: red;
        opacity: 0.8;
    }

    .g2 {
        background-color: darkblue;
        opacity: 0.8;
    }

    .g3 {
        background-color: gold;
        opacity: 0.8;
    }

    .g4 {
        background-color: dimgrey;
        opacity: 0.8;
    }

    .g5 {
        background-color: green;
        opacity: 0.8;
    }

    .g6 {
