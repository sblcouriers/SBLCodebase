@section Scripts {
    <script>

        $("#AssociateId").select2();
        SetVehicleDetail();
        jQuery('.sbl-datepicker').datepicker({
            autoclose: true,
            todayHighlight: true,
            startView: 0,
            format: "dd M yyyy"
        });
        // get vehicle
        $(document.body).on("change", "#AssociateId", function () {
            var associateId = $(this).val();
            SetVehicleDetail();
        });

        function SetVehicleDetail() {
            var associateId = $("#AssociateId").val();
            if (associateId > 0) {
                $.ajax({
                    url: "@Url.Action("GetAssociateVehicle", "admin")",
                    type: "GET",
                data: { AssociateId: associateId },
                success: function (data) {
                    $("#vehicle-rented").val(data.vehiclename);
                    $("#VehicleId").val(data.vehicleId);
                }
            });
            }
        }
        SetGrid();
        function SetGrid() {
            var cols = 30;
            var rows = 30;
            var width = 20;
            var height = 20;
            $('#side1').empty();
            for (y = 0; y <= (rows - 1) ; y++) {
                for (x = 0; x <= (cols - 1) ; x++) {
                    $('#side1').append("<div class=\"gridcell " + "x" + x + "y" + y + "\" data-cols=\"" + cols + "\" data-rows=\"" + rows + "\" data-x=\"" + x + "\" data-y=\"" + y + "\" data-width=\"" + width + "\" data-height=\"" + height + "\" style=\"width:" + (width) + "px; height:" + (height) + "px;\"></div>");
                }
            }
            $('#side2').empty();
            for (y = 0; y <= (rows - 1) ; y++) {
                for (x = 0; x <= (cols - 1) ; x++) {
                    $('#side2').append("<div class=\"gridcell " + "x" + x + "y" + y + "\" data-cols=\"" + cols + "\" data-rows=\"" + rows + "\" data-x=\"" + x + "\" data-y=\"" + y + "\" data-width=\"" + width + "\" data-height=\"" + height + "\" style=\"width:" + (width) + "px; height:" + (height) + "px;\"></div>");
                }
            }
            $('#front').empty();
            for (y = 0; y <= (rows - 1) ; y++) {
                for (x = 0; x <= (cols - 1) ; x++) {
                    $('#front').append("<div class=\"gridcell " + "x" + x + "y" + y + "\" data-cols=\"" + cols + "\" data-rows=\"" + rows + "\" data-x=\"" + x + "\" data-y=\"" + y + "\" data-width=\"" + width + "\" data-height=\"" + height + "\" style=\"width:" + (width) + "px; height:" + (height) + "px;\"></div>");
                }
            }
            $('#back').empty();
